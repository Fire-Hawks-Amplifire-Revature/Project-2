<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Customer_Rating_Range</fullName>
    <active>true</active>
    <description>Ensures the rating for customer success story &gt;=1 and &lt;=5</description>
    <errorConditionFormula>Rating__c&lt;1 || Rating__c &gt;5</errorConditionFormula>
    <errorDisplayField>Account__c</errorDisplayField>
    <errorMessage>Rating must be from 1-5</errorMessage>
</ValidationRule>
